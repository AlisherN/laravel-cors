{
    "name": "spatie/laravel-cors-lite",
    "description": "Send CORS headers in a Laravel application",
    "keywords": [
        "spatie",
        "laravel-cors-lite",
        "cors",
        "ajax",
        "request",
        "api"
    ],
    "homepage": "https://github.com/spatie/laravel-cors-lite",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "orchestra/testbench": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\CorsLite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\CorsLite\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\CorsLite\\CorsLiteServiceProvider"
            ]
        }
    }
}
